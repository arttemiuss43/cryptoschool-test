{
    "extends": "@vue/tsconfig/tsconfig.dom.json",
    "compilerOptions": {
        "strict": false,
        "allowJs": false,
        "noImplicitAny": false,
        "types": ["vite/client"],
        "paths": {
            "@/*": ["resources/js/*"]
        },
        "baseUrl": ".",
        "target": "esnext",
        "module": "esnext",
        "moduleResolution": "node"
    },
    "include": [
        "resources/**/*.ts",
        "resources/**/*.d.ts",
        "resources/**/*.vue"
    ]
}
